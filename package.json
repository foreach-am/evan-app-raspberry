{
  "name": "evan-raspberry-app",
  "version": "1.0.0",
  "private": true,
  "author": "forEach LLC",
  "license": "MIT",
  "description": "EVAN raspberry app.",
  "typings": "./global.d.ts",
  "types": "./global.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "start": "bash ./scripts/app/run-development.sh",
    "build": "bash ./scripts/app/build-production.sh",
    "restart": "bash ./scripts/app/restart-production.sh",
    "postinstall": "bash ./scripts/hooks/package/post-install.sh",
    "prepare": "npm run tool:husky:init",
    "lint": "npm run tool:lint:uncache && npm run tool:lint:execute:dev",
    "format": "npm run tool:format:code && npm run tool:format:scripts",
    "tool:app:env-check": "bash ./scripts/app/check-env.sh",
    "tool:husky:init": "bash ./scripts/tools/init-git-hooks.sh",
    "tool:sum-hash:build": "bash ./scripts/tools/sumhash-build.sh",
    "tool:sum-hash:check": "bash ./scripts/tools/sumhash-check.sh",
    "tool:lint:uncache": "bash ./scripts/tools/eslint-uncache.sh",
    "tool:lint:execute:dev": "bash ./scripts/tools/eslint-execute.sh dev",
    "tool:lint:execute:prod": "bash ./scripts/tools/eslint-execute.sh prod",
    "tool:format:code": "bash ./scripts/tools/format-code.sh",
    "tool:format:scripts": "bash ./scripts/tools/format-scripts.sh"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "onoff": "^6.0.3",
    "rc522-rfid-promise": "^16.0.1",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "serialport": "^10.4.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-modules-umd": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@types/node": "^18.7.14",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-minify": "^0.5.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "prettier-plugin-sh": "^0.11.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2",
    "typescript-transform-paths": "^3.3.1"
  }
}
